{
  "name": "@oai2lmapi/model-metadata",
  "version": "0.1.0",
  "description": "Shared model metadata registry for OAI2LMApi packages",
  "type": "commonjs",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/esm/index.mjs",
      "require": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc && tsc -p tsconfig.esm.json && node scripts/rename-esm-output.cjs",
    "clean": "rm -rf dist",
    "test": "pnpm run build && node --test test",
    "lint": "eslint src"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/hugefiver/OAI2LMApi.git",
    "directory": "packages/model-metadata"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^8.50.0",
    "@typescript-eslint/parser": "^8.50.0",
    "eslint": "^9.39.2",
    "typescript": "^5.9.3"
  }
}
